@model IEnumerable<cruzDDAC.Models.VesselTbl>

@{
    ViewBag.Title = "Index";
}

<h2>All Vessels</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vessel_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vessel_Agent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vessel_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vessel_Approval)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ScheduleTbl.Sailing_Route)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Vessel_Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vessel_Agent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vessel_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vessel_Approval)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScheduleTbl.Sailing_Route)
            </td>
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id=item.Vessel_ID }) |
                    @Html.ActionLink("Details", "Details", new { id=item.Vessel_ID }) |
                    @Html.ActionLink("Delete", "Delete", new { id=item.Vessel_ID })*@
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Return", "VesselNotify")